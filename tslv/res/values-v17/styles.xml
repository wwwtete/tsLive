<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ButtonCompat" parent="@style/ButtonCompatBase">
        <item name="android:textStyle">bold</item>
        <item name="android:background">@drawable/button_compat_shape</item>
    </style>
    <style name="ButtonCompatBase">
        <item name="android:textSize">14.0sp</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingTop">5.0dip</item>
        <item name="android:paddingBottom">5.0dip</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
        <item name="android:minWidth">88.0dip</item>
        <item name="android:minHeight">36.0dip</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:paddingStart">20.0dip</item>
        <item name="android:paddingEnd">20.0dip</item>
